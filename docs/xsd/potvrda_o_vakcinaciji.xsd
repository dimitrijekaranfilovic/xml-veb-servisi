<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    targetNamespace="https://www.bilgejc.rs/potvrda_o_vakcinaciji"
    xmlns="https://www.bilgejc.rs/potvrda_o_vakcinaciji"
    xmlns:za="https://www.bilgejc.rs/zajednicko">
    <xs:import namespace="https://www.bilgejc.rs/zajednicko" schemaLocation="zajednicko.xsd"/>
    <xs:element name="potvrda_o_vakcinaciji">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="qr_kod" type="za:TQr_kod"/>
                <xs:element name="licne_informacije" type="za:TLicni_podaci"/>
                <xs:element name="vakcinacija" type="TVakcinacija"> </xs:element>
            </xs:sequence>
            <xs:attribute name="sifra_potvrde" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="250"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="datum_izdavanja" type="xs:date" use="required"/>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="TDoze">
        <xs:sequence maxOccurs="1">
            <xs:element maxOccurs="unbounded" name="doza" type="za:TDoza"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TVakcinacija">
        <xs:sequence>
            <xs:element name="ustanova" type="za:TOgraniceni_string"/>
            <xs:element name="naziv_vakcine" type="za:TOgraniceni_string"/>
            <xs:element name="doze" type="TDoze"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
